{
  "name": "laravel-tools/lazy-dependency",
  "description": "This package provides a possibility to lazy load dependencies using attribute.",
  "homepage": "https://github.com/laravel-tools/lazy-dependency",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "orchestra/testbench": "^8.21",
    "pestphp/pest": "^2.34"
  },
  "autoload": {
    "psr-4": {
      "LaravelTools\\LazyDependency\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelTools\\LazyDependency\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
